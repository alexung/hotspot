<tr>
	<td class="contributors_images">
		<div class="top_row">
			<% repository.contributors.each do |contributor| %>
			<%= render partial: 'repositories/contributor',locals: {contributor: contributor} %>
			<% end %>
		</div>
	</td>
	<td>
		<%= render partial: 'repositories/file_paths',locals: {repository: repository} %>
	</td>
	<td>
		<div class="graph_container">
			<%= render partial: 'repositories/graph_interval' %>
			<%= render partial: 'repositories/graph_interval' %>
			<%= render partial: 'repositories/graph_interval' %>
			<%= render partial: 'repositories/graph_interval' %>
			<div class="graph_interval">
				<div class="start_project graph_segment"></div>
			</div>
		</div>
	</td>
	<%# require 'json' %>
	<%# @repo_graphs = repository. %>
	<td><%= repository.commits %></td>
	<td>
		<% parse_css_perc(JSON.parse(repository.graph_arr)).each do |values| %>
			<%#= render partial: "graph", locals: {additions_deletions: additions_deletions} %>
		<section>
	    <%#= binding.pry%>
	    <div class="additions segment_height_<%= values[1] %>"></div>
	     <div class="deletions segment_height_<%= values[2] %>"></div>
	    <div class="unchanged segment_height_<%= values[0] %>"></div>
		</section>
		<% end %>
	</td>
</tr>






